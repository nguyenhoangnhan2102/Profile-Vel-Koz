    <div class="form-add-new">
        <form action="/delete-skin" method="post" enctype="multipart/form-data">
            <fieldset>
                <legend>Delete Skin</legend>
                <div>Hello</div>
                <div class="input-group">
                    <label>ID:</label>
                    <input type="text" name="skin_Id" value="<%= skinEdit.skin_id %>" readonly>
                </div>
                <div class="input-group">
                    <label>Are you sure to detele this skin?</label>
                    <input type="text" name="skin_name" value="<%= skinEdit.tentrangphuc %>">
                </div>
                <div>
                    <button>Confirm</button>
                </div>
            </fieldset>
        </form>
    </div>

</body>

